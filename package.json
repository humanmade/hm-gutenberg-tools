{
  "name": "hm-gutenberg-tools",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "npm run clean:build && concurrently -p name -n \"JS,CSS\" -c \"cyan.bold,yellow.bold\" \"npm run js:build\" \"npm run css:build\"",
    "watch": "concurrently -p name -n \"JS,CSS\" -c \"cyan.bold,yellow.bold\" \"npm run js:watch\" \"npm run sass:watch\"",
    "dev": "npm run watch",
    "lint": "eslint ./js --fix",
    "js:build": "cross-env NODE_ENV=production webpack",
    "js:watch": "webpack --watch",
    "css:build": "npm run sass:build && npm run postcss:build",
    "postcss:build": "postcss ./build/*.css --no-map --use autoprefixer postcss-clean --replace",
    "sass:build": "node-sass-chokidar css/ -o ./build/",
    "sass:build:dev": "node-sass-chokidar --source-map true --output-style expanded css/ -o ./build/",
    "sass:watch": "npm run sass:build:dev; chokidar \"./css/**/*.scss\" -c \"npm run sass:build:dev\"",
    "clean:build": "del-cli ./build/*",
    "test": "jest"
  },
  "jest": {
    "testURL": "http://localhost",
    "setupFiles": [
      "./js/test-env.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@wordpress/data": "^2.0.0",
    "autoprefixer": "^7.2.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0-beta.0",
    "chokidar-cli": "^1.2.3",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.1",
    "cross-env": "^5.*",
    "del-cli": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-humanmade": "^0.6.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "immutability-helper": "^2.6.4",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "node-sass": "^4.13.1",
    "node-sass-chokidar": "^1.3.5",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^6.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-select": "^2.0.0",
    "react-test-renderer": "^16.4.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
