{
  "name": "hm-gutenberg-tools",
  "version": "1.0.0-beta",
  "private": true,
  "scripts": {
    "build": "yarn clean:build && concurrently -p name -n \"JS,CSS\" -c \"cyan.bold,yellow.bold\" \"yarn js:build\" \"yarn css:build\"",
    "watch": "concurrently -p name -n \"JS,CSS\" -c \"cyan.bold,yellow.bold\" \"yarn js:watch\" \"yarn sass:watch\"",
    "dev": "yarn watch",
    "lint": "eslint ./js --fix",
    "js:build": "cross-env NODE_ENV=production webpack",
    "js:watch": "webpack --watch",
    "css:build": "yarn sass:build && yarn postcss:build",
    "postcss:build": "postcss ./build/*.css --no-map --use autoprefixer postcss-clean --replace",
    "sass:build": "node-sass-chokidar css/ -o ./build/",
    "sass:build:dev": "node-sass-chokidar --source-map true --output-style expanded css/ -o ./build/",
    "sass:watch": "yarn sass:build:dev; chokidar \"./css/**/*.scss\" -c \"yarn sass:build:dev\"",
    "clean:build": "del-cli ./build/*",
    "test": "jest"
  },
  "jest": {
    "testURL": "http://localhost",
    "setupFiles": [
      "./js/test-env.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@wordpress/data": "^2.0.0",
    "autoprefixer": "^7.2.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta.0",
    "chokidar-cli": "^1.2.0",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.1",
    "cross-env": "^5.*",
    "del-cli": "^1.1.0",
    "eslint": "^5.4.0",
    "eslint-config-humanmade": "^0.5.0",
    "eslint-config-react-app": "^0.5.2",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^6.10.3",
    "immutability-helper": "^2.6.4",
    "jest": "^23.5.0",
    "jest-cli": "^21.3.0-beta.15",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^4.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-select": "^2.0.0",
    "react-test-renderer": "^16.4.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0"
  },
  "dependencies": {}
}
